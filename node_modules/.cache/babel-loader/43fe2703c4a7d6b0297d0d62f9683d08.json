rcesContent":["<template>\n  <div>\n    <v-navigation-drawer\n      v-model=\"drawer\"\n      app\n      temporary\n      dark\n      src=\"@/assets/img/bgDrawer.jpg\"\n    >\n      <!-- #!CHANGE\n      <v-list>\n        <v-list-item>\n          <v-list-item-avatar>\n            <img src=\"@/assets/img/logo.png\" alt=\"Logo\" />\n          </v-list-item-avatar>\n          <v-list-item-content>\n            <v-list-item-title class=\"title\">Thiago</v-list-item-title>\n            <v-list-item-subtitle>Amaral</v-list-item-subtitle>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list> -->\n\n      <v-divider />\n\n      <v-list dense>\n        <v-list-item\n          v-for=\"([icon, text, link], i) in items\"\n          :key=\"i\"\n          link\n          @click=\"$vuetify.goTo(link)\"\n        >\n          <v-list-item-icon class=\"justify-center\">\n            <v-icon>{{ icon }}</v-icon>\n          </v-list-item-icon>\n          <v-list-item-content>\n            <v-list-item-title class=\"subtitile-1 small-text-shadow\">{{\n              text\n            }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-app-bar\n      app\n      :color=\"color\"\n      :flat=\"flat\"\n      dark\n      class=\"px-15\"\n      :class=\"{ expand: flat }\"\n    >\n      <!-- #!CHANGE\n      <v-toolbar-title>\n        <v-img src=\"@/assets/img/logo.png\" max-width=\"50px\" />\n      </v-toolbar-title> -->\n      <v-spacer />\n      <v-app-bar-nav-icon\n        @click.stop=\"drawer = !drawer\"\n        class=\"mr-4\"\n        v-if=\"isXs\"\n      />\n      <div v-else>\n        <v-btn text @click=\"$vuetify.goTo('#home')\">\n          <span class=\"mr-2 small-text-shadow\">Home</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#about')\">\n          <span class=\"mr-2 small-text-shadow\">About</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#projects')\">\n          <span class=\"mr-2 small-text-shadow\">Projects</span>\n        </v-btn>\n        <v-btn text @click=\"$vuetify.goTo('#pricing')\">\n          <span class=\"mr-2 small-text-shadow\">Experience</span>\n        </v-btn>\n        <v-btn rounded outlined text @click=\"$vuetify.goTo('#contact')\">\n          <span class=\"mr-2 small-text-shadow\">Contact Me</span>\n        </v-btn>\n      </div>\n    </v-app-bar>\n  </div>\n</template>\n\n<style scoped>\n.small-text-shadow {\n  color: white;\n  text-shadow: 2px 2px 2px #000000;\n}\n\n.v-toolbar {\n  transition: 0.6s;\n}\n\n.expand {\n  height: 80px !important;\n  padding-top: 10px;\n}\n</style>\n\n<script>\nexport default {\n  data: () => ({\n    drawer: null,\n    isXs: false,\n    items: [\n      [\"mdi-home-outline\", \"Home\", \"#hero\"],\n      [\"mdi-information-outline\", \"About\", \"#about\"],\n      [\"mdi-download-box-outline\", \"Projects\", \"#projects\"],\n      [\"mdi-currency-usd\", \"Experience\", \"#pricing\"],\n      [\"mdi-email-outline\", \"Contact Me\", \"#contact\"],\n    ],\n  }),\n  props: {\n    color: String,\n    flat: Boolean,\n  },\n  methods: {\n    onResize() {\n      this.isXs = window.innerWidth < 850;\n    },\n  },\n\n  watch: {\n    isXs(value) {\n      if (!value) {\n        if (this.drawer) {\n          this.drawer = false;\n        }\n      }\n    },\n  },\n  mounted() {\n    this.onResize();\n    window.addEventListener(\"resize\", this.onResize, { passive: true });\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}